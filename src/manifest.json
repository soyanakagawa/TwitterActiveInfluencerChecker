{
  "name": "Twitter Active Influencer Checker",
  "description": "直近でアクティブなインフルエンサーがいるかをチェックします。",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["*://twitter.com/*"],
    "js": ["jquery-2.2.0.min.js", "ChEx.js", "content_script.js"]
  }],
  "options_page": "options.html",
  "permissions": [
    "*://twitter.com/*",
    "storage"
  ]
}
